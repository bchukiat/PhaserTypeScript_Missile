{
    "name": "rox",
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "declaration": false,
        "noImplicitAny": false,
        "removeComments": true,
        "noLib": false,
        "out": "./src/game/app.js"
    },
    "filesGlob": [
        "./**/*.ts",
        "!./node_modules/**/*.ts"
    ],
    "files": [
        "./src/game/app.ts",
        "./src/game/explosion.ts",
        "./src/game/missile.ts",
        "./src/game/missilelauncher.ts",
        "./src/game/rocket.ts",
        "./src/game/rocketlauncher.ts",
        "./src/game/silo.ts",
        "./src/sprite/city.ts",
        "./src/state/boot.ts",
        "./src/state/game.ts",
        "./src/state/mainmenu.ts",
        "./src/state/preloader.ts",
        "./src/state/rocketlauncher.ts",
        "./src/typing/p2.d.ts",
        "./src/typing/phaser.comments.d.ts",
        "./src/typing/phaser.d.ts",
        "./src/typing/pixi.comments.d.ts",
        "./src/typing/pixi.d.ts"
    ]
}
